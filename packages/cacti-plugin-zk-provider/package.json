{
  "name": "cacti-plugin-zk-provider",
  "packageManager": "yarn@4.3.1",
  "main": "dist/lib/main/typescript/index.js",
  "module": "dist/lib/main/typescript/index.js",
  "types": "dist/lib/main/typescript/index.d.ts",
  "files": [
    "dist/lib/main/**/*.js",
    "dist/lib/main/**/*.d.ts",
    "dist/lib/main/**/*.json"
  ],
  "type": "module",
  "dependencies": {
    "@ethereumjs/rlp": "^10.1.1",
    "@ethereumjs/trie": "^6.2.1",
    "@ethereumjs/util": "^10.1.1",
    "@hyperledger/cactus-common": "2.1.0",
    "@hyperledger/cactus-core": "workspace:^",
    "@vercel/ncc": "^0.38.4",
    "@zkpassport/poseidon2": "^0.6.2",
    "ethers": "^6.16.0",
    "express": "^5.2.1",
    "ganache": "^7.9.2",
    "hardhat": "^3.1.6",
    "ncc": "^0.3.6",
    "openzeppelin": "^1.0.0",
    "poseidon": "^0.3.2",
    "redis": "^5.10.0",
    "zokrates-js": "^1.1.9"
  },
  "scripts": {
    "build": "tsc",
    "build-bundle": "ncc build dist/lib/index.js -o build",
    "start": "node ./dist/lib/index.js",
    "zk-test": "npx vitest run ./src/test/typescript/vitest/zero-knowledge-handler.test.ts -t \"Import testing\"",
    "zk-test2": "npx vitest run ./src/test/typescript/vitest/zero-knowledge-handler.test.ts -t \"Prove hash knowledge\"",
    "endpoint-test": "npx vitest run ./src/test/typescript/vitest/endpoint-setup.test.ts -t \"Endpoint Setup\"",
    "zk-server-test": "npx vitest run ./src/test/typescript/vitest/zk-server.test.ts -t \"ZK Server With Endpoints\"",
    "zk-server-db-test": "npx vitest run ./src/test/typescript/vitest/zk-server.test.ts -t \"ZK Server Storing and Fetching from Database\"",
    "zk-server-db-test2": "npx vitest run ./src/test/typescript/vitest/zk-server.test.ts -t \"ZK Server automatically Fetching from Database\"",
    "zk-client-test": "npx vitest run ./src/test/typescript/vitest/zk-server.test.ts -t \"Client to Server\"",
    "zk-client-test2": "npx vitest run ./src/test/typescript/vitest/zk-server.test.ts -t \"ZK Server circuit load and validation\"",
    "zk-client-test3": "npx vitest run ./src/test/typescript/vitest/zk-server.test.ts -t \"Proving EVM transactions\"",
    "docker-rebuild": "yarn build && yarn build-bundle && yarn docker-build",
    "docker-build": "docker build -f zero-knowledge-server.Dockerfile -t zkserver .",
    "docker-network-setup": "docker network create zknet",
    "docker-run-local": "docker run -p 3000:3000 --name localserver1 --network zknet zkserver 0",
    "docker-run-offserver": "docker run -p 6379:6379 -p 3001:3001 --name offserver1 --network zknet zkserver 1",
    "forge:build:contracts": "forge build --out ./src/test/solidity/generated"
  },
  "devDependencies": {
    "@types/express": "^5",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  }
}
